{
  "name": "@grnsft/if-exhaust-plugins",
  "version": "1.0.0",
  "description": "if standard library of exhaust plugins",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && tsc --project tsconfig.build.json && npm run copy-files",
    "clean": "rimraf build/",
    "copy-files": "copyfiles -u 1 src/**/*.csv build",
    "coverage": "jest --verbose --coverage",
    "fix": "gts fix",
    "fix:package": "fixpack",
    "lint": "gts lint",
    "prepare": "husky install",
    "prepublish": "npm run build",
    "test": "jest --verbose"
  },
  "repository": {
    "url": "https://github.com/Green-Software-Foundation/if-exhaust-plugins.git"
  },
  "keywords": [
    "green software foundation",
    "if",
    "exhaust",
    "plugins"
  ],
  "author": {
    "name": "Green Software Foundation",
    "email": "info@gsf.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Green-Software-Foundation/if-exhaust-plugins/issues"
  },
  "homepage": "https://greensoftware.foundation",
  "dependencies": {
    "typescript": "^5.1.6"
  },
  "devDependencies": {
    "@babel/core": "7.22.10",
    "@babel/preset-typescript": "^7.22.5",
    "@commitlint/cli": "^18.6.0",
    "@commitlint/config-conventional": "^18.6.0",
    "@jest/globals": "^29.6.1",
    "@types/jest": "^29.5.7",
    "@types/node": "^20.4.5",
    "fixpack": "^4.0.0",
    "gts": "^5.0.0",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2"
  }
}
